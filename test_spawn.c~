#include <stdio.h>
#include <spawn.h>
#include <unistd.h>
#include <sys/types.h>
#define CLONE_SIGHAND   0x00000800
#define CLONE_THREAD    0x00010000
#include <linux/unistd.h>
#include <errno.h>

void main(){
pid_t processID;
pid_t my_processID;
extern char **environ;
//char *argV[] = {"192.168.1.40",(char *) 0};
int status = -1;
//my_processID = getpid();
//printf("Parent Process : ID %d\n",my_processID);
status = posix_spawn(&processID,"/home/user/Desktop/CS461_project/hello",NULL,NULL,NULL,environ);
if(status == 0){
     printf("Launched Application \n");
     printf("Child Process ID %d\n",processID);
     while(1);
     return;
}
else
     printf("Launching application Failed");
printf("In current application");

return;
}
